{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name"
        },
        "AzurePostgreSql1": {
            "type": "string"
        },
        "AzureBlobStorage1": {
            "type": "string"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "[concat(parameters('factoryName'), '/etl_agri_data')]",
            "type": "Microsoft.DataFactory/factories/pipelines",
            "apiVersion": "2018-06-01",
            "properties": {
                "activities": [
                    {
                        "name": "Data flow1",
                        "type": "ExecuteDataFlow",
                        "dependsOn": [],
                        "policy": {
                            "timeout": "0.12:00:00",
                            "retry": 0,
                            "retryIntervalInSeconds": 30,
                            "secureOutput": false,
                            "secureInput": false
                        },
                        "userProperties": [],
                        "typeProperties": {
                            "dataflow": {
                                "referenceName": "dataflow1",
                                "type": "DataFlowReference",
                                "parameters": {},
                                "datasetParameters": {
                                    "source1": {},
                                    "sink1": {}
                                }
                            },
                            "staging": {},
                            "compute": {
                                "coreCount": 8,
                                "computeType": "General"
                            },
                            "traceLevel": "Fine"
                        }
                    }
                ],
                "policy": {
                    "elapsedTimeMetric": {}
                },
                "annotations": [],
                "lastPublishTime": "2025-12-13T17:45:51Z"
            },
            "dependsOn": [
                "[concat(variables('factoryId'), '/dataflows/dataflow1')]"
            ]
        },
        {
            "name": "[concat(parameters('factoryName'), '/dataflow1')]",
            "type": "Microsoft.DataFactory/factories/dataflows",
            "apiVersion": "2018-06-01",
            "properties": {
                "type": "MappingDataFlow",
                "typeProperties": {
                    "sources": [
                        {
                            "dataset": {
                                "referenceName": "DelimitedText1",
                                "type": "DatasetReference"
                            },
                            "name": "source1"
                        }
                    ],
                    "sinks": [
                        {
                            "linkedService": {
                                "referenceName": "[parameters('AzurePostgreSql1')]",
                                "type": "LinkedServiceReference"
                            },
                            "name": "sink1"
                        }
                    ],
                    "transformations": [
                        {
                            "name": "select1"
                        }
                    ],
                    "scriptLines": [
                        "source(output(",
                        "          {Code Domaine} as string,",
                        "          Domaine as string,",
                        "          {Code zone (M49)} as string,",
                        "          Zone as string,",
                        "          {Code Élément} as string,",
                        "          {Élément} as string,",
                        "          {Code Produit (CPC)} as double,",
                        "          Produit as string,",
                        "          {Code année} as integer,",
                        "          {Année} as integer,",
                        "          {Code Mois} as integer,",
                        "          Mois as string,",
                        "          {Unité} as string,",
                        "          Valeur as double,",
                        "          Symbole as string,",
                        "          {Description du Symbole} as string",
                        "     ),",
                        "     allowSchemaDrift: false,",
                        "     validateSchema: false,",
                        "     ignoreNoFilesFound: false) ~> source1",
                        "source1 select(mapColumn(",
                        "          Zone,",
                        "          Element = {Élément},",
                        "          Produit,",
                        "          Annee = {Année},",
                        "          Mois,",
                        "          Unite = {Unité},",
                        "          Valeur,",
                        "          {Code Mois}",
                        "     ),",
                        "     partitionBy('hash', 1),",
                        "     skipDuplicateMapInputs: true,",
                        "     skipDuplicateMapOutputs: true) ~> select1",
                        "select1 sink(allowSchemaDrift: true,",
                        "     validateSchema: false,",
                        "     format: 'table',",
                        "     tableName: 'faostat_data',",
                        "     schemaName: 'public',",
                        "     deletable: false,",
                        "     insertable: true,",
                        "     updateable: false,",
                        "     upsertable: false,",
                        "     truncate: true,",
                        "     store: 'postgres') ~> sink1"
                    ]
                }
            },
            "dependsOn": [
                "[concat(variables('factoryId'), '/datasets/DelimitedText1')]"
            ]
        },
        {
            "name": "[concat(parameters('factoryName'), '/DelimitedText1')]",
            "type": "Microsoft.DataFactory/factories/datasets",
            "apiVersion": "2018-06-01",
            "properties": {
                "linkedServiceName": {
                    "referenceName": "[parameters('AzureBlobStorage1')]",
                    "type": "LinkedServiceReference"
                },
                "annotations": [],
                "type": "DelimitedText",
                "typeProperties": {
                    "location": {
                        "type": "AzureBlobStorageLocation",
                        "fileName": "FAOSTAT_data_fr_12-12-2025.csv",
                        "folderPath": "faostat-data",
                        "container": "agridata"
                    },
                    "columnDelimiter": ",",
                    "escapeChar": "\\",
                    "firstRowAsHeader": true,
                    "quoteChar": "\""
                },
                "schema": [
                    {
                        "name": "Code Domaine",
                        "type": "String"
                    },
                    {
                        "name": "Domaine",
                        "type": "String"
                    },
                    {
                        "name": "Code zone (M49)",
                        "type": "String"
                    },
                    {
                        "name": "Zone",
                        "type": "String"
                    },
                    {
                        "name": "Code Élément",
                        "type": "String"
                    },
                    {
                        "name": "Élément",
                        "type": "String"
                    },
                    {
                        "name": "Code Produit (CPC)",
                        "type": "String"
                    },
                    {
                        "name": "Produit",
                        "type": "String"
                    },
                    {
                        "name": "Code année",
                        "type": "String"
                    },
                    {
                        "name": "Année",
                        "type": "String"
                    },
                    {
                        "name": "Code Mois",
                        "type": "String"
                    },
                    {
                        "name": "Mois",
                        "type": "String"
                    },
                    {
                        "name": "Unité",
                        "type": "String"
                    },
                    {
                        "name": "Valeur",
                        "type": "String"
                    },
                    {
                        "name": "Symbole",
                        "type": "String"
                    },
                    {
                        "name": "Description du Symbole",
                        "type": "String"
                    }
                ]
            },
            "dependsOn": []
        }
    ]
}